{
  "name": "zombiebox",
  "version": "2.0.0",
  "description": "ZombieBox is a JavaScript framework for development of Smart TV and STB applications",
  "homepage": "http://zombiebox.tv",
  "author": {
    "name": "Interfaced",
    "email": "info@interfaced.ru"
  },
  "license": "MIT",
  "keywords": [
    "framework",
    "Smart TV",
    "SmartTV",
    "STB"
  ],
  "files": [
    "bin",
    "lib",
    "templates",
    "zb",
    "CHANGELOG.md",
    "README.md"
  ],
  "engines": {
    "node": ">=8.9",
    "npm": ">=5"
  },
  "main": "./lib/index.js",
  "module": "./zb/",
  "bin": {
    "zombiebox": "./bin/cli.js",
    "zb": "./bin/cli.js"
  },
  "scripts": {
    "test": "npm run test.framework && npm run test.tools",
    "test.framework": "karma start ./test/framework/karma.conf.js",
    "test.tools": "mocha ./test/tools/suites --exit",
    "lint": "eslint zb lib bin test scripts",
    "generateDocs": "node ./scripts/generate-config-docs.js && eslint lib/config/interface.js --fix"
  },
  "dependencies": {
    "archiver": "^3.0.0",
    "chalk": "^2.4.2",
    "chokidar": "3.0.1",
    "connect": "3.7.0",
    "espree": "^5.0.1",
    "fs-extra": "^8.0.1",
    "google-closure-compiler": "^20190528.0.0",
    "http-proxy": "1.17.0",
    "jsonschema": "^1.2.4",
    "klaw-sync": "^6.0.0",
    "lodash": "^4.17.11",
    "morgan": "1.9.1",
    "postcss": "^7.0.16",
    "postcss-csso": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "postcss-url": "^8.0.0",
    "semver": "^6.1.1",
    "send": "^0.17.1",
    "serve-static": "1.14.1",
    "yargs": "^13.2.4",
    "zb-log-server": "0.0.6"
  },
  "devDependencies": {
    "ast-types": "^0.13.1",
    "chai": "^4.2.0",
    "escodegen": "^1.11.1",
    "eslint": "^5.16.0",
    "eslint-config-interfaced": "^1.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-interfaced": "^1.6.1",
    "eslint-plugin-node": "^9.1.0",
    "got": "^9.6.0",
    "json-schema-ref-parser": "^6.1.0",
    "karma": "^4.1.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-module-resolver-preprocessor": "^1.1.1",
    "mocha": "^6.1.4",
    "mock-require": "^3.0.3",
    "portfinder": "^1.0.20",
    "sinon": "^7.3.2",
    "strip-ansi": "^5.2.0",
    "tmp": "^0.1.0",
    "zombiebox-platform-pc": "^2.0.0"
  },
  "peerDependencies": {
    "zombiebox-platform-pc": "^2.0.0"
  }
}
