{
  "name": "zombiebox",
  "version": "2.2.1",
  "description": "ZombieBox is a JavaScript framework for development of Smart TV and STB applications",
  "homepage": "http://zombiebox.tv",
  "repository": {
    "type": "git",
    "url": "git@github.com:interfaced/zombiebox.git"
  },
  "author": {
    "name": "Interfaced",
    "email": "info@interfaced.ru"
  },
  "license": "MIT",
  "keywords": [
    "framework",
    "Smart TV",
    "SmartTV",
    "STB"
  ],
  "files": [
    "bin",
    "docs",
    "lib",
    "templates",
    "vendor",
    "zb",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "engines": {
    "node": ">=8.9",
    "npm": ">=5"
  },
  "main": "./lib/index.js",
  "module": "./zb/",
  "bin": {
    "zombiebox": "./bin/cli.js",
    "zb": "./bin/cli.js"
  },
  "scripts": {
    "test": "npm run test.framework && npm run test.tools",
    "test.framework": "karma start ./test/framework/karma.conf.js",
    "test.tools": "mocha ./test/tools/suites --exit",
    "lint": "eslint .",
    "generateDocs": "node ./scripts/generate-config-docs.js && eslint lib/config/interface.js --fix"
  },
  "dependencies": {
    "archiver": "^3.1.1",
    "chalk": "^2.4.2",
    "chokidar": "3.1.0",
    "connect": "3.7.0",
    "espree": "^6.1.1",
    "fs-extra": "^8.1.0",
    "google-closure-compiler": "^20190909.0.0",
    "http-proxy": "1.18.0",
    "jsonschema": "^1.2.4",
    "klaw-sync": "^6.0.0",
    "lodash": "^4.17.15",
    "morgan": "1.9.1",
    "postcss": "^7.0.18",
    "postcss-csso": "^3.0.0",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-url": "^9.0.0",
    "postcss-values-parser": "^3.0.5",
    "semver": "^6.3.0",
    "send": "^0.17.1",
    "serve-static": "1.14.1",
    "yargs": "^14.1.0",
    "zb-log-server": "0.0.6"
  },
  "devDependencies": {
    "ast-types": "^0.13.2",
    "chai": "^4.2.0",
    "escodegen": "^1.12.0",
    "eslint": "^6.4.0",
    "eslint-config-interfaced": "2.0.0-beta.3",
    "eslint-import-resolver-zombiebox": "1.0.0",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-interfaced": "^2.0.0-beta.1",
    "eslint-plugin-jsdoc": "^15.9.2",
    "eslint-plugin-node": "^10.0.0",
    "got": "^9.6.0",
    "json-schema-ref-parser": "^7.1.1",
    "karma": "^4.3.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-module-resolver-preprocessor": "^1.1.1",
    "mocha": "^6.2.0",
    "mock-require": "^3.0.3",
    "portfinder": "^1.0.24",
    "sinon": "^7.4.2",
    "strip-ansi": "^5.2.0",
    "tmp": "^0.1.0",
    "zombiebox-platform-pc": "^2.1.0"
  },
  "peerDependencies": {
    "zombiebox-platform-pc": "^2.1.0"
  }
}
